<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:text="00:00"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Matcha"
        android:layout_width="107dp"
        android:layout_height="107dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/Menta"
        app:layout_constraintEnd_toStartOf="@+id/Blanco"
        app:layout_constraintStart_toEndOf="@+id/Verde"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/matcha"
        tools:srcCompat="@drawable/matcha" />

    <ImageView
        android:id="@+id/Blanco"
        android:layout_width="108dp"
        android:layout_height="107dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/blanco"
        tools:srcCompat="@drawable/blanco" />

    <ImageView
        android:id="@+id/Verde"
        android:layout_width="109dp"
        android:layout_height="107dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="150dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/verde"
        tools:srcCompat="@drawable/verde" />

    <ImageView
        android:id="@+id/Manzanilla"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Verde"
        app:srcCompat="@drawable/manzanilla"
        tools:srcCompat="@drawable/manzanilla" />

    <ImageView
        android:id="@+id/Menta"
        android:layout_width="107dp"
        android:layout_height="109dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/Hibisco"
        app:layout_constraintStart_toEndOf="@+id/Manzanilla"
        app:layout_constraintTop_toBottomOf="@+id/Matcha"
        app:srcCompat="@drawable/menta"
        tools:srcCompat="@drawable/menta" />

    <ImageView
        android:id="@+id/Hibisco"
        android:layout_width="108dp"
        android:layout_height="109dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Blanco"
        app:srcCompat="@drawable/hibisco"
        tools:srcCompat="@drawable/hibisco" />

</androidx.constraintlayout.widget.ConstraintLayout>